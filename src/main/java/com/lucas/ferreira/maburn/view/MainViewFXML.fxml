<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/11.0.1">
	<children>
		<GridPane fx:id="menuGridPane" alignment="CENTER"
			gridLinesVisible="true" hgap="15" styleClass="menu" vgap="0">


			<padding>
				<Insets top="50" right="10" bottom="10" left="10" />
			</padding>





			<Button fx:id="btnConfig"
				onAction="#onClickButtonConfiguration" text="Configuration"
				GridPane.columnIndex="7" GridPane.rowIndex="0">


				<GridPane.margin>
					<Insets right="250" />
				</GridPane.margin>

			</Button>


			<Button fx:id="btnHome" onAction="#onClickButtonHome"
				text="Home" GridPane.columnIndex="5" GridPane.rowIndex="0">

			</Button>

			<Button fx:id="btnExtra" onAction="#onClickButtonHome"
				text="Downloads" GridPane.columnIndex="3" GridPane.rowIndex="0">
				<GridPane.margin>
					<Insets left="250" />
				</GridPane.margin>
			</Button>




		</GridPane>
	</children>

</fx:root>
